<Application x:Class="HangMan.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:HangMan"
             xmlns:vm="clr-namespace:HangMan.ViewModels"
             StartupUri="Views/LoginView.xaml">
    <Application.Resources>
        <vm:LoginViewModel x:Key="LoginViewModel"/>
        <vm:NewUserViewModel x:Key="NewUserViewModel"/>
        <vm:GameViewModel x:Key="GameViewModel"/>
        <vm:AboutViewModel x:Key="AboutViewModel"/>
    </Application.Resources>
</Application>
